{
	"$schema": "https://json.schemastore.org/claude-code-settings.json",
	"permissions": {
		"allow": ["Bash(tree:*)", "Skill(skill-creator)"],
		"deny": [
			"Bash(git reset --hard:*)",
			"Bash(npm i --global:*)",
			"Bash(npm i -g:*)",
			"Bash(npm install --global:*)",
			"Bash(npm install -g:*)",
			"Bash(npm run dev:*)",
			"Bash(npm run serve:*)",
			"Bash(npm run start &)",
			"Bash(npm run start:*)",
			"Bash(npx:*)",
			"Bash(op)",
			"Bash(op:*)",
			"Bash(pip:*)",
			"Bash(pip3:*)",
			"Bash(open:*)",
			"Bash(* ./cmd/migrate *)"
		]
	}
}
